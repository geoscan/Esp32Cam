<?xml version="1.0"?>

<PropertyList>

<generic>
	<output>
		<binary_footer>magic,0x5F534746</binary_footer>
		<binary_mode>true</binary_mode>
		<byte_order>host</byte_order>

		<chunk>
			<name>latitude</name>
			<type>double</type>
			<node>/systems/autopilot/telemetry/latitude-deg</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>longitude</name>
			<type>double</type>
			<node>/systems/autopilot/telemetry/longitude-deg</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>heading</name>
			<type>float</type>
			<node>/orientation/track-deg</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>yaw</name>
			<type>float</type>
			<node>/orientation/heading-deg</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>accel_x</name>
			<type>float</type>
			<node>/accelerations/pilot/x-accel-fps_sec</node>
			<factor>0.0311</factor>  <!-- fp/s2 to m/s2 -->
		</chunk>

		<chunk>
			<name>accel_y</name>
			<type>float</type>
			<node>/accelerations/pilot/y-accel-fps_sec</node>
			<factor>0.0311</factor>  <!-- fp/s2 to m/s2 -->
		</chunk>

		<chunk>
			<name>accel_z</name>
			<type>float</type>
			<node>/accelerations/pilot/z-accel-fps_sec</node>
			<factor>0.0311</factor>  <!-- fp/s2 to m/s2 -->
		</chunk>

		<chunk>
			<name>roll_rate</name>
			<type>float</type>
			<node>/systems/autopilot/telemetry/roll-rate-degps</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>pitch_rate</name>
			<type>float</type>
			<node>/systems/autopilot/telemetry/pitch-rate-degps</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>yaw_rate</name>
			<type>float</type>
			<node>/systems/autopilot/telemetry/yaw-rate-degps</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>magnetic_heading</name>
			<type>float</type>
			<node>/systems/autopilot/telemetry/heading-magnetic-deg</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>magnetic-dip</name>
			<type>float</type>
			<node>/environment/magnetic-dip-deg</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>voltage</name>
			<type>float</type>
			<node>/systems/autopilot/telemetry/voltage</node>
		</chunk>

		<chunk>
			<name>current</name>
			<type>float</type>
			<node>/systems/autopilot/telemetry/load_amps</node>
		</chunk>

		<chunk>
			<name>groundspeed</name>
			<type>float</type>
			<node>/systems/autopilot/telemetry/groundspeed-kt</node>
			<factor>0.514444</factor>  <!-- knot to m/s -->
		</chunk>

		<chunk>
			<name>satellites</name>
			<type>int</type>
			<node>/systems/autopilot/telemetry/satellites</node>
		</chunk>

		<chunk>
			<name>dynamic_pressure</name>
			<type>float</type>
			<node>/systems/autopilot/telemetry/dynamic-pressure-inhg</node>
			<factor>3386.389</factor>  <!-- inhg to pa -->
		</chunk>

		<chunk>
			<name>static-pressure</name>
			<type>float</type>
			<node>/systems/autopilot/telemetry/pressure-inhg</node>
			<factor>3386.389</factor>  <!-- inhg to pa -->
		</chunk>

		<chunk>
			<name>pitch</name>
			<type>float</type>
			<node>/systems/autopilot/telemetry/pitch-deg</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>roll</name>
			<type>float</type>
			<node>/systems/autopilot/telemetry/roll-deg</node>
			<factor>1</factor>
		</chunk>
	</output>

	<input>
		<line_separator>newline</line_separator>
		<var_separator>,</var_separator>

		<chunk>
			<name>aileron</name>
			<type>float</type>
			<node>/systems/autopilot/controls/aileron</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>aileron-trim</name>
			<type>float</type>
			<node>/systems/autopilot/controls/aileron-trim</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>elevator</name>
			<type>float</type>
			<node>/systems/autopilot/controls/elevator</node>
			<factor>-1</factor>
		</chunk>

		<chunk>
			<name>elevator-trim</name>
			<type>float</type>
			<node>/systems/autopilot/controls/elevator-trim</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>rudder</name>
			<type>float</type>
			<node>/systems/autopilot/controls/rudder</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>rudder-trim</name>
			<type>float</type>
			<node>/systems/autopilot/controls/rudder-trim</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>throttle</name>
			<type>float</type>
			<node>/systems/autopilot/controls/throttle</node>
			<factor>1</factor>
		</chunk>

		<chunk>
			<name>parachute</name>
			<type>float</type>
			<node>/systems/parachute/servo-pos</node>
		</chunk>

		<chunk>
			<name>state</name>
			<type>int</type>
			<node>/systems/autopilot/state</node>
		</chunk>
	</input>
</generic>

</PropertyList>
